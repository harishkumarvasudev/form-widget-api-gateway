# must have all the variables in present in .env 
APP_NAME=formwidget-dashboard
APP_ENV=development
PORT=9990

#enable https
HTTPS='false'       #false or true, if set to true, then use https in url

#log
LOG_LEVEL=debug  # in development set to 'debug' , in production set it 'info' (without quotes)
LOG_FILE=log.txt
LOG_FORMAT=common
LOG_SIZE=10M
LOG_INTERVAL=1d



#JWT #needs to be same as in API-gateway
JWT_TOKEN_SECRET=
JWT_EXPIRY_TIME=900s